<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<context-param>
		<param-name>app-name</param-name>
		<param-value>Chats</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>users</servlet-name>
		<servlet-class>com.revature.servlets.UserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>updateRequest</servlet-name>
		<servlet-class>com.revature.servlets.UpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>managerRequest</servlet-name>
		<servlet-class>com.revature.servlets.ManagerRequestsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>requestReimbursements</servlet-name>
		<servlet-class>com.revature.servlets.RequestsReimbursementServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>submitreimbursement</servlet-name>
		<servlet-class>com.revature.servlets.SubmitsReimbursementServlet</servlet-class>
	</servlet>
	
	
	
	<servlet-mapping>
	<servlet-name>updateRequest</servlet-name>
	<url-pattern>/updateRequest</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>users</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>
		
		<servlet-mapping>
		<servlet-name>managerRequest</servlet-name>
		<url-pattern>/manager</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>requestReimbursements</servlet-name>
		<url-pattern>/request</url-pattern>
	</servlet-mapping>
	 
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
		<servlet-name>submitreimbursement</servlet-name>
		<url-pattern>/submitreimbursement</url-pattern>
	</servlet-mapping>

	
</web-app>
